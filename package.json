{
  "name": "skintest-starter",
  "version": "0.0.0",
  "engines": {
    "node": ">=14"
  },
  "scripts": {
    "build:watch": "npm run build -- --watch",
    "build": "npm run clean && tsc --build",
    "clean": "rimraf dist",
    "lint": "eslint src",
    "start:watch": "nodemon --watch dist --no-stdin --delay 1 dist/skintest.js",
    "start": "node dist/skintest.js"
  },
  "devDependencies": {
    "@types/node": "15.6.1",
    "@typescript-eslint/eslint-plugin": "4.25.0",
    "@typescript-eslint/parser": "4.25.0",
    "eslint": "7.27.0",
    "nodemon": "^2.0.7",
    "rimraf": "^3.0.2",
    "typescript": "4.3.2"
  },
  "dependencies": {
    "@skintest/common": "^1.0.7-alpha.0",
    "@skintest/platform": "^1.0.7-alpha.0",
    "@skintest/plugins": "^1.0.7-alpha.0",
    "@skintest/sdk": "^1.0.7-alpha.0"
  }
}
